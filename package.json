{
  "name": "generator-alo-ng",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0"
  },
  "description": "Yeoman generator for an Angular 4 module",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf generators/**/*.js util/**/*.js generators/**/*.js.map util/**/*.js.map typings",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "cross-spawn": "^5.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "pad": "^2.0.1",
    "tslib": "^1.8.0",
    "yeoman-generator": "^2.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.80",
    "@types/yeoman-generator": "^2.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.5.3"
  }
}
