{
  "name": "generator-alo-ng",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0"
  },
  "description": "Yeoman generator for an Angular 4 module",
  "main": "generators/app/index.js",
  "types": "types/generators/app/index.d.ts",
  "scripts": {
    "clean": "rimraf generators/app/*.js generators/app/*.js.map util/**/*.js.map util/**/*.js types ./*.tgz",
    "prereinstall": "rm -rf node_modules package-lock.json",
    "reinstall": "npm install",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prebuild:prod": "npm run clean",
    "build:prod": "tsc -p tsconfig.prod.json",
    "prewatch": "npm run clean",
    "watch": "tsc --watch"
  },
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "angular",
    "aot"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "cross-spawn": "^5.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "pad": "^2.0.1",
    "tslib": "^1.8.0",
    "yeoman-generator": "^2.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.80",
    "@types/yeoman-generator": "^2.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.5.3"
  }
}
